{
    "name": "okaspar/nova-translatable",
    "description": "A Laravel Nova field for spatie/laravel-translatable forked from mrmonat/nova-translatable to enable RTL support",
    "keywords": [
        "laravel",
        "nova",
        "translatable"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translatable": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "okaspar\\Translatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "okaspar\\Translatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
